{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:recommended", "airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/extensions": ["error", "ignorePackages"],
    "operator-linebreak": "off",
    "implicit-arrow-linebreak": "off",
    "no-param-reassign": "off",
    "function-paren-newline": "off",
    "object-curly-newline": "off"
  }
}
